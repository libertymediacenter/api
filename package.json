{
  "name": "liberty-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "yarn run tsc",
    "test": "yarn run test:lint && yarn run test:unit",
    "test:unit": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --recursive",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "typeorm": "$(yarn bin)/cli.js",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon",
    "start:prod": "cross-env NODE_ENV=production ./node_modules/.bin/pm2-runtime dist/index.js",
    "start:debug": "NODE_ENV=production UV_THREADPOOL_SIZE=128 NODE_DEBUG=http,fs node --title=LibertyAPI --inspect dist/index.js",
    "start:worker": "ts-node src/worker.ts",
    "start:worker:dev": "nodemon -e ts --watch 'src/**/*.ts' --exec ts-node -- ./src/Worker.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/common": "^5.2.2",
    "@tsed/core": "^5.2.2",
    "@tsed/di": "^5.2.2",
    "@tsed/swagger": "^5.2.2",
    "@tsed/testing": "^5.2.2",
    "@tsed/typeorm": "^5.2.2",
    "@types/cote": "^0.19.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bottleneck": "^2.17.0",
    "compression": "^1.7.3",
    "concurrently": "^4.1.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cote": "^0.19.1",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^7.0.1",
    "got": "^9.6.0",
    "helmet": "^3.15.1",
    "ioredis": "^4.6.2",
    "klaw": "^3.0.0",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "mime-types": "^2.1.22",
    "moment": "^2.24.0",
    "node-uuid": "^1.4.8",
    "pg": "^7.8.1",
    "pm2": "^3.3.1",
    "query-string": "^6.2.0",
    "redis-commands": "^1.4.0",
    "sharp": "^0.21.3",
    "slug": "^1.0.0",
    "ts-json-properties": "^2.0.3",
    "typeorm": "^0.2.14"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/fluent-ffmpeg": "^2.1.9",
    "@types/fs-extra": "^5.0.5",
    "@types/got": "^9.4.1",
    "@types/helmet": "^0.0.42",
    "@types/http-proxy": "^1.16.2",
    "@types/ioredis": "^4.0.8",
    "@types/klaw": "^3.0.0",
    "@types/lodash": "^4.14.121",
    "@types/mime-types": "^2.1.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.5",
    "@types/query-string": "^6.2.0",
    "@types/request-promise": "^4.1.42",
    "@types/sharp": "^0.21.2",
    "@types/sinon": "^7.0.7",
    "@types/sinon-chai": "^3.2.2",
    "@types/slug": "^0.9.1",
    "@types/swagger-schema-official": "^2.0.15",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.0.2",
